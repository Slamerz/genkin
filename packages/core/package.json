{
	"name": "@genkin/core",
	"version": "0.1.0",
	"description": "Core currency and number formatting/math library for Genkin",
	"type": "module",
	"main": "dist/index.cjs",
	"module": "dist/index.js",
	"types": "dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js",
			"require": "./dist/index.cjs"
		}
	},
	"files": [
		"dist",
		"README.md"
	],
	"scripts": {
		"build": "tsup",
		"dev": "tsup --watch",
		"test": "vitest run"
	},
	"keywords": [
		"currency",
		"money",
		"formatting",
		"decimal",
		"arithmetic",
		"typescript"
	],
	"author": "Genkin Team",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/genkin/genkin",
		"directory": "packages/core"
	},
	"bugs": {
		"url": "https://github.com/genkin/genkin/issues"
	},
	"homepage": "https://genkin.dev",
	"sideEffects": false,
	"publishConfig": {
		"access": "public"
	}
}
