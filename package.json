{
  "name": "genkin",
  "version": "0.0.1",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./core": {
      "types": "./dist/core.d.ts",
      "import": "./dist/core.js",
      "require": "./dist/core.cjs"
    },
    "./operations": {
      "types": "./dist/operations.d.ts",
      "import": "./dist/operations.js",
      "require": "./dist/operations.cjs"
    },
    "./formatters": {
      "types": "./dist/formatters.d.ts",
      "import": "./dist/formatters.js",
      "require": "./dist/formatters.cjs"
    },
    "./currencies": {
      "types": "./dist/currencies.d.ts",
      "import": "./dist/currencies.js",
      "require": "./dist/currencies.cjs"
    },
    "./utils": {
      "types": "./dist/utils.d.ts",
      "import": "./dist/utils.js",
      "require": "./dist/utils.cjs"
    },
    "./dinero": {
      "types": "./dist/dinero-v1.d.ts",
      "import": "./dist/dinero-v1.js",
      "require": "./dist/dinero-v1.cjs"
    },
    "./dineroV2": {
      "types": "./dist/dinero-v2.d.ts",
      "import": "./dist/dinero-v2.js",
      "require": "./dist/dinero-v2.cjs"
    }
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "bundle:analyze": "bundlemon --config bundlemon.config.js",
    "bundle:size": "bun run build && bun run scripts/bundle-size.ts"
  },
  "devDependencies": {
    "@dinero.js/currencies": "^2.0.0-alpha.14",
    "@types/bun": "latest",
    "@types/gzip-size": "^5.1.1",
    "@types/node": "^24.4.0",
    "bundlemon": "^3.1.0",
    "dinero.js": "^2.0.0-alpha.14",
    "dinero-og-v1": "npm:dinero.js@^1.9.1",
    "gzip-size": "^7.0.0",
    "tsup": "^8.5.1",
    "vitest": "^3.2.4"
  },
  "peerDependencies": {
    "typescript": "^5.8.3"
  }
}