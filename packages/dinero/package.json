{
	"name": "@genkin/dinero",
	"version": "0.1.0",
	"description": "Dinero.js v1 compatible API built on Genkin core",
	"type": "module",
	"main": "dist/index.cjs",
	"module": "dist/index.js",
	"types": "dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js",
			"require": "./dist/index.cjs"
		}
	},
	"files": [
		"dist",
		"README.md"
	],
	"scripts": {
		"build": "tsup",
		"dev": "tsup --watch",
		"test": "vitest run"
	},
	"keywords": [
		"dinero",
		"money",
		"currency",
		"finance",
		"typescript",
		"migration",
		"genkin"
	],
	"author": "Slamerz",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/Slamerz/genkin",
		"directory": "packages/dinero"
	},
	"bugs": {
		"url": "https://github.com/Slamerz/genkin/issues"
	},
	"homepage": "https://slamerz.github.io/genkin/",
	"sideEffects": false,
	"peerDependencies": {
		"@genkin/core": "^0.1.1"
	},
	"publishConfig": {
		"access": "public"
	}
}
